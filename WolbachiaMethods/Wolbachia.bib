abstract = {Here we report evidence of a mitochondrial-Wolbachia sweep in North American populations of the parasitoid wasp Nasonia vitripennis, a cosmopolitan species and emerging model organism for evolutionary and genetic studies. Analysis of the genetic variation of 89 N. vitripennis specimens from Europe and North America was performed using four types of genetic markers: a portion of the mitochondrial cytochrome oxidase I gene, nine polymorphic nuclear microsatellites, sequences from 11 single-copy nuclear markers and six Wolbachia genes. The results show that the European populations have a sevenfold higher mitochondrial sequence variation than North American populations, but similar levels of microsatellite and nuclear gene sequence variation. Variation in the North American mitochondria is extremely low (pi=0.31%), despite a highly elevated mutation rate (similar to 35-40 times higher than the nuclear genes) in the mitochondria of Nasonia. The data are indicative of a mitochondrial sweep in the North American population, possibly due to Wolbachia infections that are maternally co-inherited with the mitochondria. Owing to similar levels of nuclear variation, the data could not resolve whether N. vitripennis originated in the New or the Old World. Heredity (2010) 104, 318-326; doi:10.1038/hdy.2009.160; published online 20 January 2010},
url = {http://www.nature.com/hdy/journal/v104/n3/abs/hdy2009160a.html},
local-url = {file://localhost/Users/Chris/Documents/Papers2/Articles/2010/Raychoudhury/Raychoudhury_2010_Heredity.pdf},
file = {{Raychoudhury_2010_Heredity.pdf:/Users/Chris/Documents/Papers2/Articles/2010/Raychoudhury/Raychoudhury_2010_Heredity.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/hdy.2009.160}}
}

@article{Batista:2010p8194,
author = {Batista, Philip D and Keddie, B Andrew and Dosdall, Lloyd M and Harris, Harriet L},
title = {{Phylogenetic placement and evidence for horizontal transfer of Wolbachia in Plutella xylostella (Lepidoptera: Plutellidae) and its parasitoid, Diadegma insulare (Hymenoptera: Ichneumonidae)}},
