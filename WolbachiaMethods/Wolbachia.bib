pages = {1081--1090},
affiliation = {Laboratoire G{\'e}n{\'e}tique et Biologie des Populations de Crustac{\'e}s, Universit{\'e} de Poitiers, CNRS UMR 6556, France. didier.bouchon@campus.univ-poitiers.fr},
