@article{Zabalou:2004kx,
author = {Zabalou, Sofia and Charlat, Sylvain and Nirgianaki, Androniki and Lachaise, Daniel and Mercot, Herve and Bourtzis, Kostas},
title = {{Natural Wolbachia infections in the Drosophila yakuba species complex do not induce cytoplasmic incompatibility but fully rescue the wRi modification.}},
journal = {Genetics},
