file = {{Miller_2006_Appl_Environ_Microb.pdf:/Users/Chris/Documents/Papers2/Articles/2006/Miller/Miller_2006_Appl_Environ_Microb.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1128/AEM.72.1.826-835.2006}}
}

@article{Sakamoto:2007eq,
