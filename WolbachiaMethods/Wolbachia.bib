}

@article{Jaenike:2010p8195,
author = {Jaenike, John and Stahlhut, Julie K and Boelio, Lisa M and Unckless, Robert L},
title = {{Association between Wolbachia and Spiroplasma within Drosophila neotestacea: an emerging symbiotic mutualism}},
journal = {Molecular Ecology},
year = {2010},
volume = {19},
number = {2},
pages = {414--425},
affiliation = {Univ Rochester, Dept Biol, Rochester, NY 14627 USA},
doi = {10.1111/j.1365-294X.2009.04448.x},
language = {English},
read = {Yes},
rating = {0},
date-added = {2010-04-16T14:37:26GMT},
date-modified = {2016-01-23T18:09:13GMT},
abstract = {Interspecific mutualism can evolve when specific lineages of different species tend to be associated with each other from one generation to the next. Different maternally transmitted endosymbionts occurring within the same cytoplasmic lineage fulfil this requirement. Drosophila neotestacea is infected with maternally transmitted Wolbachia and Spiroplasma, which are cotransmitted at high frequency in natural populations. Molecular phylogenetic evidence indicates that both endosymbionts have been present in D. neotestacea for considerable evolutionary periods. Thus, conditions are suitable for the evolution of mutualism between them. In support of this possibility, there is a significant positive association between Wolbachia and Spiroplasma infection in many samples of D. neotestacea from natural populations. Theoretically, such a positive association can result from either mutualism between these endosymbionts or recent spread. Collections from present-day populations suggest that recent spread and mutualism have both operated to generate the positive association between Wolbachia and Spiroplasma. If selection acts on the combination of these two endosymbionts, they may be in the early stages of evolution of a more complex, cooperative association.},
url = {http://www3.interscience.wiley.com/journal/123194223/abstract},
local-url = {file://localhost/Users/Chris/Documents/Papers2/Articles/2010/Jaenike/Jaenike_2010_Molecular_Ecology.pdf},
