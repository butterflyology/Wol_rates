@article{Min:1997vf,
author = {Min, K T and Benzer, S},
title = {{Wolbachia, normally a symbiont of Drosophila, can be virulent, causing degeneration and early death.}},
journal = {Proceedings of the National Academy of Sciences},
year = {1997},
volume = {94},
number = {20},
pages = {10792--10796},
