@article{Braig:1998p4070,
author = {Braig, H R and Zhou, W and Dobson, S L and O'Neill, S L},
title = {{Cloning and characterization of a gene encoding the major surface protein of the bacterial endosymbiont Wolbachia pipientis}},
journal = {Journal of Bacteriology},
year = {1998},
volume = {180},
number = {9},
pages = {2373--2378},
month = may,
