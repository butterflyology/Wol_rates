journal = {Insect Biochemistry and Molecular Biology},
year = {2010},
volume = {40},
number = {12},
pages = {847--854},
month = dec,
affiliation = {Graduate School of Agricultural and Life Sciences, The University of Tokyo, Yayoi 1-1-1, Bunkyo-ku, Tokyo 113-8657, Japan.},
doi = {10.1016/j.ibmb.2010.08.004},
pmid = {20728536},
language = {English},
rating = {0},
date-added = {2014-06-17T02:56:50GMT},
date-modified = {2016-04-25T16:23:44GMT},
abstract = {A homologue of the sex-determining gene doublesex, Osdsx, was identified in the adzuki bean borer Ostrinia scapulalis. Three isoforms of the Osdsx transcript (Osdsx(M), Osdsx(FL) and Osdsx(FS)) differing in length were found. Osdsx(M) was specifically found in males, and contained an 852-bp ORF encoding a protein of 284 amino acids. Osdsx(FL) and Osdsx(FS) were found in females, and had the same 813-bp ORF encoding a protein of 271 amino acids. The Osdsx gene was inferred to have six exons and five introns. The variation in the transcript could be explained by the alternative splicing of Osdsx: Osdsx(M) was formed by the splicing of exons 1, 2, 5 and 6, Osdsx(FS) by the splicing of exons 1-4 and 6, and Osdsx(FL) by the splicing of exons 1-6. RT-PCR analysis indicated that Osdsx was transcribed in a sex-specific manner in all somatic tissues examined, regardless of developmental stage. In Wolbachia-induced sexual mosaics of O. scapulalis, which are genetically male, the female-specific isoform of Osdsx (Osdsx(FL)) was shown to be expressed in addition to the male-specific isoform (Osdsx(M)). This finding provides the first evidence that Wolbachia manipulates the sex of its host by interfering either with the sex-specific splicing of Osdsx itself or with another upstream sex determination process.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20728536&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Chris/Documents/Papers2/Articles/2010/Sugimoto/Sugimoto_2010_Insect_Biochem_Mol_Biol.pdf},
file = {{Sugimoto_2010_Insect_Biochem_Mol_Biol.pdf:/Users/Chris/Documents/Papers2/Articles/2010/Sugimoto/Sugimoto_2010_Insect_Biochem_Mol_Biol.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.ibmb.2010.08.004}}
}

