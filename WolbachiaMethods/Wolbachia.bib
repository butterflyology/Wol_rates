pages = {257--266},
month = jan,
affiliation = {Institute of Dental Research, Westmead Centre For Oral Health, Westmead Hospital, PO Box 533, Wentworthville, NSW 2145, Australia. mnadkarni@dental.wsahs.nsw.gov.au},
