	Journal = {Molecular Biology and Evolution},
	Number = {2},
	Pages = {437--449},
	Rating = {0},
	Read = {Yes},
	Title = {{Widespread recombination throughout \emph{Wolbachia} genomes.}},
	Volume = {23},
	Year = {2006},
