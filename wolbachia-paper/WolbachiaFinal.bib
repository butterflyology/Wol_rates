	Journal = {Molecular Ecology},
	Number = {2},
	Pages = {286--298},
	Rating = {0},
	Title = {{Multiple \emph{Wolbachia} determinants control the evolution of cytoplasmic incompatibilities in \emph{Culex pipiens} mosquito populations.}},
