	Journal = {Molecular Ecology},
	Number = {18},
	Pages = {3816--3830},
	Rating = {0},
	Read = {Yes},
	Title = {{Hidden \emph{Wolbachia} diversity in field populations of the European cherry fruit fly, \emph{Rhagoletis cerasi} (Diptera, Tephritidae)}},
