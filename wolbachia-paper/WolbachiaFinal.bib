	Journal = {Applied And Environmental Microbiology},
	Number = {1},
	Pages = {826--835},
	Rating = {0},
	Read = {Yes},
	Title = {{Evolutionary dynamics of wAu-like \emph{Wolbachia} variants in neotropical \emph{Drosophila} spp.}},
	Volume = {72},
	Year = {2006},
